<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{ISO8601}. [%thread] %level %logger %msg%n
            </pattern>
        </encoder>
    </appender>

    <!--<root level="info">-->
    <root level="warn">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>